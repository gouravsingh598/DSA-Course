pair<long long, long long> getMinMax(long long arr[], int n) {
    
     pair<long long int,long long int> p;
     long long int temp=10000000000000;
     long long int temp1=0;
    for(int i=0;i<n;i++)
    {
        if(arr[i]<temp)
        {
          temp=arr[i];
        }
        
        if(arr[i]>temp1)
        temp1=arr[i];
    }
   
    p.first=temp;
    p.second=temp1;
    return p;
}
